<odoo>
    <data>
        <record id="view_stock_picking_total_product_form" model="ir.ui.view">
            <field name="name">stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[1]/field[@name='move_line_nosuggest_ids']" position="after">
                    <br/>
                    <label for="total_purchase_quantity" string="Total Quantity:"/>
                    <field name="total_delivered_qty"/>
                </xpath>
                <xpath expr="//page[2]/field[@name='move_line_ids_without_package']" position="after">
                    <br/>
                    <label for="total_purchase_quantity" string="Total Quantity:"/>
                    <field name="total_delivered_qty"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>