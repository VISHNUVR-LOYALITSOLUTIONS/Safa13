<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_purchaseorder_quotation_document_inherit" inherit_id="purchase.report_purchasequotation_document">
        <xpath expr="//thead/tr/th[@name='th_quantity']" position="before">
            <th name="th_mrp"><strong>MRP</strong></th>
            <th name="th_margin_per"><strong>Margin %</strong></th>
        </xpath>

        <xpath expr="//tbody/t/tr/t[@t-if='not order_line.display_type']/td[3]" position="before">
            <td class="text-right">
                <span t-field="order_line.mrp"/>
            </td>
            <td class="text-center">
                <span t-field="order_line.margin_percentage"/>%
            </td>
        </xpath>
    </template>



</odoo>